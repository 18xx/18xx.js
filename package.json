{
  "name": "18xx-tile.js",
  "version": "0.0.1",
  "engines": {
    "node": "6.11.1"
  },
  "description": "An 18xx Board Game Tile Generator",
  "main": "index.js",
  "devDependencies": {
    "codecov": "^2.3.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "ts-jest": "^20.0.10",
    "tslint": "^5.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/definitions/**/*"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "mapCoverage": true,
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "scripts": {
    "assets": "webpack --config ./webpack.dev.config.js --watch",
    "cov": "jest --coverage --no-cache",
    "heroku-postbuild": "webpack --config ./webpack.prod.config.js --progress",
    "lint": "tslint --project .",
    "test": "jest",
    "start": "nf start"
  },
  "author": "Gregor MacDougall",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cookie-session": "^2.0.32",
    "@types/ejs": "^2.3.33",
    "@types/enzyme": "^2.8.4",
    "@types/express": "^4.0.36",
    "@types/jest": "^20.0.6",
    "@types/lodash": "^4.14.72",
    "@types/passport": "^0.3.3",
    "@types/passport-google-oauth": "^1.0.35",
    "@types/react": "^15.6.0",
    "@types/react-dom": "^15.5.2",
    "@types/react-test-renderer": "^15.5.2",
    "@types/redis": "^2.6.0",
    "@types/whatwg-fetch": "^0.0.33",
    "awesome-typescript-loader": "^3.2.2",
    "aws-sdk": "^2.95.0",
    "body-parser": "^1.17.2",
    "bootstrap": "4.0.0-alpha.6",
    "cookie-session": "^2.0.0-beta.2",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.7",
    "enzyme": "^2.9.1",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "foreman": "^2.0.0",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "source-map-loader": "^0.2.1",
